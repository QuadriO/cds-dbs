{
  "name": "cds-dbs",
  "version": "1.0.0",
  "description": "This is a monorepo for our SQL Database Services.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@capire/sflight": "sap-samples/cap-sflight",
    "axios": ">=1.3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "express": "^4",
    "jest": "^29"
  },
  "scripts": {
    "prettier": "npx prettier --write .",
    "test": "jest --silent",
    "test:all": "npm run test:none && npm run test:sqlite && npm run test:pg",
    "test:none": "DB=none npm run test",
    "test:sqlite": "DB=sqlite npm run test",
    "test:pg": "npm run pg:up && DB=pg npm run test",
    "lint": "npx eslint . && npx prettier --check ."
  },
  "workspaces": [
    "core-sql",
    "postgres",
    "sqlite"
  ],
